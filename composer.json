{
    "name": "softfineware/openapi-php-dto-generator",
    "description": "A basic template for creating php packages with basic setup: psalm, php-cs-fixer, phpunit, CI with running test",
    "type": "project",
    "autoload": {
        "psr-4": {
            "SoftFineWare\\OpenapiGenerator\\": "src"
        }
    },
    "minimum-stability": "stable",
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test"
        }
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Legion112",
            "email": "ageev.maks2014@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^8.2",
        "nette/php-generator": "^4",
        "php-di/php-di": "^6.3",
        "monolog/monolog": "^3",
        "symfony/yaml": "^6",
        "nette/utils": "^3.2",
        "symfony/console": "^6",
        "phpspec/prophecy": "^1.3",
        "symfony/dependency-injection": "^6.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.23",
        "phpunit/phpunit": "^10",
        "roave/security-advisories": "dev-latest"
    }
}
