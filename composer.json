{
    "name": "homeapp/php-skeleton",
    "description": "A basic template for creating php packages with basic setup: psalm, php-cs-fixer, phpunit, CI with running test",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Homeapp\\OpenapiGenerator\\": "src"
        }
    },
    "minimum-stability": "stable",
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test"
        }
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Legion112",
            "email": "ageev.maks2014@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.4",
        "nette/php-generator": "^3.5",
        "symfony/console": "^5.3",
        "php-di/php-di": "^6.3",
        "monolog/monolog": "^2.3",
        "symfony/yaml": "^5.3"
    },
    "require-dev": {
        "vimeo/psalm": "^4.7",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest"
    }
}
